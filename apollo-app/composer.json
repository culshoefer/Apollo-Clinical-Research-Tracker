{
  "require": {
    "doctrine/orm": "*",
    "philo/laravel-blade": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.7.*",
    "fzaninotto/faker": "^1.5"
  },
  "minimum-stability": "dev",
  "autoload": {
    "files": [
      "apollo/Constants.php"
    ],
    "psr-0": {
      "": "apollo/entities"
    },
    "psr-4": {
      "Apollo\\": "apollo",
      "Apollo\\Components\\": "apollo/components",
      "Apollo\\Controllers\\": "apollo/controllers",
      "Apollo\\Entities\\": "apollo/entities",
      "Apollo\\Helpers\\": "apollo/helpers",
      "Apollo\\Tests\\": "tests",
      "Apollo\\Tests\\Components\\": "tests/components",
      "Apollo\\Tests\\Helpers\\": "tests/helpers"
    }
  }
}
